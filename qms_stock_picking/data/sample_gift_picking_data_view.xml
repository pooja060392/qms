<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="sample_wh_order_sequence" model="ir.sequence">
            <field name="name">Sample Out sequence</field>
            <field name="code">wh.order.sample</field>
            <field name="prefix">SAMPLE/OUT/</field>
            <field name="padding">4</field>
        </record>

        <record id="sample_in_sequence" model="ir.sequence">
            <field name="name">Sample In sequence</field>
            <field name="code">order.sample</field>
            <field name="prefix">SAMPLE/IN/</field>
            <field name="padding">4</field>
        </record>

<!--        <record id="gcrm_sequence" model="ir.sequence">-->
<!--            <field name="name">GCRM sequence</field>-->
<!--            <field name="code">order.sample</field>-->
<!--            <field name="prefix">GCRM/OUT</field>-->
<!--            <field name="padding">4</field>-->
<!--        </record>-->

        <record id="stock_location_sample_order_wh" model="stock.location">
            <field name="name">Sample Location</field>
            <!--<field name="location_id" ref="stock.stock_location_locations"/>-->
            <field name="usage">internal</field>
            <field name="company_id"></field>
        </record>

        <record id="sample_order_picking_type" model="stock.picking.type">
            <field name="name">Sample Order</field>
            <field name="sequence_id" ref="sample_wh_order_sequence"/>
            <field name="code">outgoing</field>
            <field name="default_location_src_id" ref="stock_location_sample_order_wh"/>
            <field name="use_create_lots" eval="True"/>
        </record>


        <record id="sample_order_return_picking_type" model="stock.picking.type">
            <field name="name">Sample Order Return</field>
            <field name="sequence_id" ref="sample_in_sequence"/>
            <field name="code">incoming</field>
            <field name="default_location_dest_id" ref="stock_location_sample_order_wh"/>
            <field name="use_create_lots" eval="True"/>
        </record>

    </data>
</odoo>