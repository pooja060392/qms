<odoo>
    <data>

        <record id="view_picking_form_inherit_warehouse" model="ir.ui.view">
            <field name="name">stock.picking.form.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref='stock.view_picking_form'/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="warehouse_type" groups="base.group_no_one"/>
                    <field name="quality_location" groups="base.group_no_one"/>
                </xpath>
            </field>
        </record>

        <record id="inherit_view_picking_type_form_warehouse_type" model="ir.ui.view">
            <field name="name">stock.picking.type.form</field>
            <field name="model">stock.picking.type</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
                <field name="default_location_src_id" position="before">
                    <field name="warehouse_type"
                           groups="base.group_no_one"
                    />
                    <field name="quality_location" groups="base.group_no_one"/>
                </field>
            </field>
        </record>

        <!--HO ACtions-->

        <record id="action_picking_ho_delivery" model="ir.actions.act_window">
            <field name="name">Delivery Orders</field>
            <field name="res_model">stock.picking</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,kanban,form,calendar</field>
            <field name="domain">[('related_code', '=', 'outgoing'), ('warehouse_type', '=', '3')]</field>
            <field name="search_view_id" ref="stock.view_picking_internal_search"/>
        </record>

        <record id="action_picking_ho_receipts" model="ir.actions.act_window">
