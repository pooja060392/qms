<odoo>
    <data>
        <record id="inherit_view_picking_type_form" model="ir.ui.view">
            <field name="name">stock.picking.type.form</field>
            <field name="model">stock.picking.type</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
                <field name="default_location_src_id" position="before">
                    <field name="base_stock_picking_type"
                           groups="base.group_no_one"
                           attrs="{'invisible':[('code', '!=', 'incoming')]}"
                    />
                    <field name="is_manufacturing"
                           groups="base.group_no_one"
                           attrs="{'invisible':[('code', '!=', 'internal')]}"
                    />
                </field>
                <field name="warehouse_id" position="after">
                    <field name="related_delivery_location_id" attrs="{'invisible': [('code', '!=', 'incoming')]}"
                           options="{'no_create': True, 'no_open': True}"/>
                </field>
            </field>
        </record>
    </data>
</odoo>