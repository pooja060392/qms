<odoo>
    <data>
<!-- Wizard form view -->
    <record id="view_send_mmo_form" model="ir.ui.view">
        <field name="name">days.fill.mo</field>
        <field name="model">days.fill.mo</field>
        <field name="arch" type="xml">
        <form string="Users">
            <sheet>
                <group>
                    <group>
                        <field name="mo_user_id" required="1" domain="[('is_mo_days','=',True)]"/>
                    </group>
                </group>
            </sheet>
        <footer>
            <button name="submit_button" string="Submit" type="object" class="btn-primary"/>
            <button string="Cancel" class="btn-default" special="cancel" />
        </footer>
        </form>
        </field>
        </record>

        <record id="action_send_mo" model="ir.actions.act_window">
            <field name="name">Send Quotation For Fill Packaging Days</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                </p>
            </field>
        </record>
    </data>
</odoo>