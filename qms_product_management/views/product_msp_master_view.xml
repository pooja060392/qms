<odoo>
    <data>

        <record id="view_msp_master_form" model="ir.ui.view">
            <field name="name">product.msp.master.form</field>
            <field name="model">product.msp.master</field>
<!--            <field name="type">form</field>-->
            <field name="arch" type="xml">
                <form create="false" delete="false">
                    <sheet>
                        <group  colspan="2">
                            <group>
                                <field name="name" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_msp_master_non_tree" model="ir.ui.view">
            <field name="name">product.msp.master.tree</field>
            <field name="model">product.msp.master</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="MSP Master" create="false" delete="false" >
                    <field name="name" />
                    <field name="active" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="action_product_msp_master" model="ir.actions.act_window">
            <field name="name">MSP Master</field>
            <field name="res_model">product.msp.master</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{'create': False, 'delete': False}</field>
            <field name="view_id" eval="False"/>
        </record>

        <record id="action_product_msp_master_tree" model="ir.actions.act_window.view">
            <field name="sequence" eval="1"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="qms_product_management.view_msp_master_non_tree"/>
            <field name="act_window_id" ref="action_product_msp_master"/>
        </record>

         <menuitem
                 id="product_msp_menu"
                 name="MSP Master"
                 action="action_product_msp_master"
                 parent="base.menu_custom"
                 sequence="3"/>

    </data>
</odoo>