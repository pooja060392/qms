<?xml version="1.0"?>
<odoo>

    <record id="view_cancel_move_wizard"  model="ir.ui.view">
        <field name="name">cancel.move.wizard.form</field>
        <field name="model">cancel.move.wizard</field>
        <field name="arch" type="xml">
            <form string="Cancel Moves">
                <p class="oe_grey">
                All selected moves will be cancelled.</p>
                <footer>
                    <button name="cancel_move" string="Cancel Moves" type="object"
                            class="btn-primary"/>
                    <button string="Close" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_cancel_move_wizrd" model="ir.actions.act_window">
        <field name="name">Cancel Move</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">cancel.move.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

